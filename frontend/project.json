{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "frontend",
  "sourceRoot": "frontend/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite",
        "cwd": "frontend"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b && vite build",
        "cwd": "frontend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint .",
        "cwd": "frontend"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite preview",
        "cwd": "frontend"
      }
    },
    "tsc": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "frontend"
      }
    }
  },
  "tags": []
}
