{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "format": {
      "dependsOn": [
        "^format"
      ]
    },
    "start": {
      "dependsOn": [
        "^start"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": true
    },
    "tsc": {
      "dependsOn": [
        "^tsc"
      ]
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "cache": true
    },
    "test:cov": {
      "dependsOn": [
        "^test:cov"
      ]
    }
  }
}