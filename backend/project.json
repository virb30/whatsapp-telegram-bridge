{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "backend",
  "sourceRoot": "backend/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest build",
        "cwd": "backend"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start",
        "cwd": "backend"
      }
    },
    "start:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start --watch",
        "cwd": "backend"
      }
    },
    "start:debug": {
      "executor": "nx:run-commands",
      "options": {
        "command": "nest start --debug --watch",
        "cwd": "backend"
      }
    },
    "start:prod": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node dist/main",
        "cwd": "backend"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "cwd": "backend"
      }
    },
    "tsc": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -p tsconfig.build.json --noEmit",
        "cwd": "backend"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest --config ./test/jest-e2e.json",
        "cwd": "backend"
      }
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest --watch",
        "cwd": "backend"
      }
    },
    "test:cov": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest --coverage",
        "cwd": "backend"
      }
    },
    "test:debug": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "cwd": "backend"
      }
    },
    "test:e2e": {
      "executor": "nx:run-commands",
      "options": {
        "command": "jest --config ./test/jest-e2e.json",
        "cwd": "backend"
      }
    }
  },
  "tags": []
}
